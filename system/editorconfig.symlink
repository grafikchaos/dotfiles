# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = false

# Indentation override for all JS under lib directory
[lib/**.js]
indent_style = space
indent_size = 2
